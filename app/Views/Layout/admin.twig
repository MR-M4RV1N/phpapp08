<!DOCTYPE html>
<html>
<head>
  {% include '/Layout/admin_parts/head.twig' %}
</head>

<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  {% include '/Layout/admin_parts/header.twig' %}
  {% include '/Layout/admin_parts/aside.twig' %}

<!-- ================================== -->
  <!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        {% block page %}{% endblock %}
        <small>{% block description %}{% endblock %}</small>
      </h1>
    </section>

    <!-- Main content -->
    <section class="content">

      <!-- Default box -->
      <div class="box">
        <div class="box-header with-border">
          <h3 class="box-title">{% block title %}Title{% endblock %}</h3>

          <div class="box-tools pull-right">
            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                    title="Collapse">
              <i class="fa fa-minus"></i></button>
            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
              <i class="fa fa-times"></i></button>
          </div>
        </div>
        <div class="box-body">
          {% block content %}{% endblock %}
        </div>
        <!-- /.box-body -->
      </div>
      <!-- /.box -->

    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->
<!-- ================================== -->

  {% include '/Layout/admin_parts/footer.twig' %}

</div>
<!-- ./wrapper -->

{% include '/Layout/admin_parts/scripts.twig' %}

</body>
</html>
